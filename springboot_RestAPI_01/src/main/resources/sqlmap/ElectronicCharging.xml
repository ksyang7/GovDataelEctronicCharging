<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace=:"com.professional.yang.mapper.ElectronicChargingMapper">
	<insert id="insertElectronicCharging" parameterType="com.professional.yang.dao.ElectronicChargingDTO">
		INSERT INTO ELECTRONIC_CHARGING(
			 CHARGING_ID
			,CHARGING_NAME
			,CHARGING_ADDR
			,CHARGING_ADDR_DETAIL
			,CHARGING_LONGITUDE
			,CHARGING_LATITUDE
			,CHARGING_USAGE_TIME
		) VALUES {
			 #{CHARGING_ID}
			,#{CHARGING_NAME}
			,#{CHARGING_ADDR}
			,#{CHARGING_ADDR_DETAIL}
			,#{CHARGING_LONGITUDE}
			,#{CHARGING_LATITUDE}
			,#{CHARGING_USAGE_TIM}		
		}
	</insert>
	
	<select id="getElectronicChargingList" resultType="com.professional.yang.dao.ElectronicChargingDTO">
		SELECT CHARGING_ID
					,CHARGING_NAME
					,CHARGING_ADDR
					,CHARGING_ADDR_DETAIL
					,CHARGING_LONGITUDE
					,CHARGING_LATITUDE
					,CHARGING_USAGE_TIME
		 FROM ELECTRONIC_CHARGING
	</select>
</mapper>